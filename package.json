  "scripts": {
    "build": "npm run clear && NODE_ENV=production webpack",
    "clear": "rm -rf public/*",
    "deploy": "npm run build && firebase deploy",
    "fix": "prettier --write .",
    "get-tweets": "ts-node scripts/get-tweets.ts",
    "lighthouse": "lighthouse-ci http://localhost:5000/ --performance=70 --pwa=85 --accessibility=95 --best-practice=100 --seo=100",
    "lint:eslint": "eslint . --ext .ts",
    "lint:prettier": "prettier --check .",
    "lint:stylelint": "stylelint \"src/**/*.scss\"",
    "lint": "concurrently npm:lint:*",
    "start:prod": "npm run build && firebase emulators:start",
    "start": "firebase emulators:exec \"webpack --watch\"",
    "test": "npm run build && firebase emulators:exec \"npm run lighthouse\""
  },
